<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/lonnlogo"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/adView">

            <ImageView
                android:id="@+id/icoroom"
                android:layout_width="match_parent"
                android:layout_height="167dp"
                android:layout_marginTop="100dp"
                app:srcCompat="@drawable/icooo"
                tools:srcCompat="@drawable/icooo" />

            <TextView
                android:id="@+id/troom"
                android:layout_width="match_parent"
                android:layout_height="108dp"
                android:layout_marginTop="20dp"
                android:fontFamily="serif"
                android:text="Room_payload5"
                android:textAlignment="center"
                android:textColor="#DC9141"
                android:textSize="30sp"
                android:textStyle="bold"
                android:typeface="monospace" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lay1"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:background="@drawable/lonn1"
            android:baselineAligned="false"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/lay2"
            app:layout_constraintStart_toEndOf="@+id/lay3"
            app:layout_constraintTop_toBottomOf="@+id/adView"
            app:layout_constraintVertical_bias="0.0">

            <Switch
                android:id="@+id/pointtt"
                android:layout_width="115dp"
                android:layout_height="match_parent"
                android:onClick="sww"
                android:text="المسابقات"
                android:textAlignment="center"
                android:textOff="المسابقات"
                android:textOn="الصفحه الرائسيه" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lay2"
            android:layout_width="45dp"
            android:layout_height="35dp"
            android:background="@drawable/lonn1"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/adView"
            app:layout_constraintVertical_bias="0.0">

            <Button
                android:id="@+id/backk"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/abc_vector_test"
                android:backgroundTint="#1BBDED"
                android:onClick="backkk"
                android:scrollIndicators="end"
                app:rippleColor="@color/cardview_light_background"
                tools:ignore="OnClick,PrivateResource"
                tools:layout_editor_absoluteY="358dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lay3"
            android:layout_width="80dp"
            android:layout_height="35dp"
            android:background="@drawable/lonn2"
            android:backgroundTintMode="add"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/adView"
            app:layout_constraintVertical_bias="0.0">

            <TextView
                android:id="@+id/pon"
                android:layout_width="41dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:fontFamily="monospace"
                android:nestedScrollingEnabled="false"
                android:text="0"
                android:textAlignment="center"
                android:textSize="14sp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/icostar"
                android:layout_width="31dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/starr"
                tools:srcCompat="@drawable/starr" />

        </LinearLayout>

        <Button
            android:id="@+id/sing"
            android:layout_width="75dp"
            android:layout_height="45dp"
            android:onClick="sing"
            android:text="اشتراك"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/pass"
            app:layout_constraintTop_toBottomOf="@+id/pass"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/reff"
            android:layout_width="55dp"
            android:layout_height="45dp"
            android:backgroundTint="#00FFFFFF"
            android:onClick="refrsh"
            android:textAlignment="center"
            app:icon="@android:drawable/stat_notify_sync"
            app:iconPadding="16dp"
            app:iconTint="#61FD00"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/sing"
            app:layout_constraintHorizontal_bias="0.96"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/pass"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/pass"
            android:layout_width="150dp"
            android:layout_height="41dp"
            android:ems="10"
            android:hint="كلمه السر"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/addd"
            app:layout_constraintTop_toBottomOf="@+id/namm"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/namm"
            android:layout_width="150dp"
            android:layout_height="41dp"
            android:ems="10"
            android:hint="الاسم"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/addd"
            app:layout_constraintTop_toTopOf="@+id/addd"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/addd"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#79F102"
            android:backgroundTintMode="src_over"
            android:onClick="div"
            android:text="اضغط لزياده النقط"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/listvv"
            app:layout_constraintVertical_bias="0.0"
            app:rippleColor="@color/design_default_color_secondary"
            app:strokeColor="#FFFFFF" />

        <ListView
            android:id="@+id/listvv"
            android:layout_width="wrap_content"
            android:layout_height="335dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lay3"
            app:layout_constraintVertical_bias="0.0" />

        <com.google.android.gms.ads.AdView
            android:id="@+id/adView"
            android:layout_width="0dp"
            android:layout_height="58dp"
            app:adSize="SMART_BANNER"
            app:adUnitId="ca-app-pub-6645174391699661/6247017419"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="OnClick" />

        <TextView
            android:id="@+id/sleepp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/namm"
            app:layout_constraintHorizontal_bias="0.52"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/namm"
            app:layout_constraintVertical_bias="0.120000005" />

        <WebView
            android:id="@+id/wwee"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="30dp"
            android:background="@drawable/lonn3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lay3"
            app:layout_constraintVertical_bias="0.0"
            tools:visibility="gone">

        </WebView>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <com.google.android.material.bottomappbar.BottomAppBar
        android:id="@+id/bottomApp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        app:fabCradleMargin="10dp"
        app:fabCradleRoundedCornerRadius="10dp"
        app:fabCradleVerticalOffset="10dp"
        tools:ignore="BottomAppBar,MissingConstraints"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="671dp" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomAppv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        app:layout_anchor="@+id/bottomApp"
        app:layout_anchorGravity="center"
        app:menu="@menu/listbt" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/bt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="bt_Home"
        android:src="@drawable/ic_home"

        app:layout_anchor="@id/bottomApp"
        tools:ignore="MissingConstraints,OnClick"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <ProgressBar
        android:id="@+id/wit"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_anchor="@+id/troom"
        app:layout_anchorGravity="top|center" />

</androidx.coordinatorlayout.widget.CoordinatorLayout>